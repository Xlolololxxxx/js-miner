{
  "manifest_version": 3,
  "name": "JS Miner Chrome",
  "version": "1.0.0",
  "description": "Chrome extension port of the JS Miner Burp extension",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "downloads"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "JS Miner"
  }
}
